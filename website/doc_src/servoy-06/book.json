{
    "title": "Data Sutra",
    "Description": "Data Sutra developer documentation",
    "root": "./",
    "plugins": [ "-sharing", "versions", "todo", "github", "prism", "-highlight", "youtube", "simple-page-toc",
          "theme-vuejs@git+https://github.com/pearofducks/gitbook-plugin-theme-vuejs.git", "-fontsettings" ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/datamosaic/data-sutra"
        },
        "simple-page-toc": {
            "maxDepth": 4,
            "skipFirstH1": true
        },
        "versions": {
            "gitbookConfigURL": "./gitbook/book.json",
            "options": [
                {
                    "value": "documentation/servoy-06/",
                    "text": "Servoy 6",
                    "selected": true
                },
                {
                    "value": "documentation/servoy-08/",
                    "text": "Servoy 8"
                }
            ]
        }
    }
}